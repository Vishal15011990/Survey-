    <div class="modal " id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
         aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <h4 class="modal-title w-100">Sign up</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true" class="spancls">&times;</span>
                    </button>
                </div>
                <span class="SpanCls2" id="Required"></span>
                <div class="modal-body row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextCompany" />
                            <label>Company Name:</label>
                            <span class="SpanCls" id="lblTxtCompany"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextDesignation" />
                            <label>Designation:</label>
                            <span class="SpanCls" id="lblDesignation"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextUserName" maxlength="7" />
                            <label>User Name:</label>
                            <span class="SpanCls" id="lblUserName"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextFirstName" />
                            <label>First Name:</label>
                            <span class="SpanCls" id="lblFirstName"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextLastName" />
                            <label>Last Name:</label>
                            <span class="SpanCls" id="lblLastName"></span>
                        </div>
                        <div class="form-group">
                            <input type="password" class="form-control" id="TextPassword" onkeyup="checkpassword();" maxlength="16" />
                            <label>Password:</label>
                            <div id="progressBar"></div>
                            <span class="SpanCls" id="lblPassword"></span>
                        </div>
                        <div class="form-group">
                            <input type="password" class="form-control" id="TextCnfPassword" onchange="onchangeCnfpwd()" />
                            <label>Confirm Password:</label>
                            <span class="SpanCls" id="lblCnfPassword"></span>
                        </div>
                    </div>


                    <div class="col-lg-6">
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextPhone" maxlength="10" />
                            <label>Phone:</label>
                            <span class="SpanCls" id="lblPhone"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextEmailId" onblur="EmailIdValidate()" />
                            <label>Email Id:</label>
                            <span class="SpanCls" id="lblEmailId"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextAddress" />
                            <label>Address:</label>
                            <span class="SpanCls" id="lblAddress"></span>
                        </div>

                        <div class="form-group">
                            @Html.DropDownListFor(model => model.Country_Info.Country_Id, ViewBag.Country as SelectList, "---Select Country---", htmlAttributes: new { @class = "form-control", @id = "Country" })
                            <label>Country:</label>
                            <span class="SpanCls" id="lblCountry"></span>
                        </div>
                        <div class="form-group">
                            @Html.DropDownListFor(model => model.State_Info.State_Id, new SelectList(Enumerable.Empty<SelectListItem>(), "State_Id", "State_Name"), "---Select State---", htmlAttributes: new { @class = "form-control", @id = "State" })
                            <label>State:</label>
                            <span class="SpanCls" id="lblState"></span>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" id="TextCity" />
                            <label>City:</label>
                            <span class="SpanCls" id="lblCity"></span>
                        </div>

                        <div class="form-group">
                            <input type="text" class="form-control" pattern="[0-9]*" maxlength="6" required name="zip" id="zip" />
                            <label>Pincode:</label>
                            <span class="SpanCls" id="lblPincode"></span>
                        </div>
                    </div>
                </div>
                <div class="modal-footer d-flex justify-content-center">
                    <button class="btn btn-deep-orange" type="button" id="btnSubmit">Register</button>
                    <button class="btn btn-deep-orange" type="button" id="btCancel">Cancel</button>
                </div>
            </div>
        </div>
    </div>




