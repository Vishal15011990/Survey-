    function onchangeCnfpwd() {
        var CnfPassword = $("#TextCnfPassword").val();
        var Password = $("#TextPassword").val();
        if (Password != CnfPassword || CnfPassword != Password) {
            $("#lblCnfPassword").html("Enter Password Carefully Password Mismathced").show().fadeOut(2000);
            $("#TextCnfPassword").val("");
            $("#TextCnfPassword").css("border-color", "Red");
        }
        else {
            $("#TextCnfPassword").css("border-color", "Green");
        }
    }






    function EmailIdValidate() {
        var emailId = $("#TextEmailId").val();
        var regex = /^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
        var blockemail = /^([\w-\.]+@@(?!gmail.com)(?!yahoo.com)(?!hotmail.com)((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?))$/;
        if (!regex.test(emailId)) {
            $("#lblEmailId").html("Email Id Is Not Valid").show().fadeOut(1000);
            $("#TextEmailId").val("");
            $("#TextEmailId").css("border-color", "Red");
        }
        else if (!blockemail.test(emailId)) {
            $("#lblEmailId").html("Please Provide Your Official Email-Id").show()
            $("#TextEmailId").val("");
            $("#TextEmailId").css("border-color", "Red");
        }
       else  {
            $("#TextEmailId").css("border-color", "Green");
        }
    }

   // $("#TextPassword").keyup(function () {
    function checkpassword() {

        var passWord = $("#TextPassword").val();
        var passLength = passWord.length;
        var numberGenerator = 0;
        var total = 0;
        var Number = /([0-9])/;
        var Alphabets = /([a-zA-Z])/;
        var symbol = /([~,!,#,@@,$,%,^,&,*,-,_,+,=,?,>,<])/;

        if (passLength <= 4) {
            numberGenerator = 38;
            total = numberGenerator;
            if (total < 90) {
                $("#progressBar").html("Password Is Weak").show().fadeIn(4000);
                $("#progressBar").css('background-color', 'darkred');
                $("#progressBar").css('width', total + '%');

            }
        }
        else {

            if (passLength < 16 && Alphabets.test(passWord) && Number.test(passWord) && symbol.test(passWord)) {

                $("#progressBar").html("Password Is Strong And Safe To Use").show().fadeIn(4000);
                $("#progressBar").css('background-color', 'green');
                $("#progressBar").css('color', 'white');
                $("#progressBar").css('width', '90%');
            }
            else {
                $("#progressBar").html("Medium Password Use Alphabet Symbols ").show().fadeIn(4000);
                $("#progressBar").css('background-color', 'orange');
                $("#progressBar").css('color', 'white');
                $("#progressBar").css('width', '70%');

            }
        }
    }

